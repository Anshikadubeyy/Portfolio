  <script>
    function sendwhatsapp() {
      // Get the user's input from the form fields (if applicable)
      const name = document.querySelector("input[placeholder='Name']").value;
      const email = document.querySelector("input[placeholder='Email']").value;
      const subject = document.querySelector(
        "input[placeholder='Subject']"
      ).value;
      const message = document.querySelector("textarea").value;

      // Construct the pre-filled WhatsApp message, including user's details (if applicable)
      const whatsappMessage = `Hello,\n\n${name} (${email}) has sent you a message through the website contact form:\n\nSubject: ${subject}\n\n${message}`;

      // Create the WhatsApp link with your phone number and pre-filled message
      const whatsappLink = `https://wa.me/917715804199?text=${encodeURIComponent(
        whatsappMessage
      )}`;

      // Redirect the user to the WhatsApp link
      window.location.href = whatsappLink;
    }
  </script>

  <script
    src="https://cdnjs.cloudflare.com/ajax/libs/typed.js/2.1.0/typed.umd.js"
